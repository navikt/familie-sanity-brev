    "sanity": "4.18.0",
    "styled-components": "6.3.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.1",
    "@sanity/eslint-config-studio": "5.0.2",
    "@typescript-eslint/eslint-plugin": "8.53.0",
    "@typescript-eslint/parser": "8.53.0",
    "eslint": "9.39.1",
