{
  "root": true,
  "project": {
    "name": "Brevgenerator"
  },
  "__experimental_spaces": [
    {
      "name": "production",
      "title": "Prod",
      "default": true,
      "api": {
        "projectId": "xsrv1mh6",
        "dataset": "production"
      }
    },
    {
      "name": "testdata",
      "title": "Test",
      "api": {
        "projectId": "xsrv1mh6",
        "dataset": "testdata"
      }
    }
  ],
  "api": {
    "projectId": "xsrv1mh6",
    "dataset": "testdata"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "barcode-input",
    "markdown",
    "@sanity/dashboard"
  ],
  "env": {
    "development": {
      "plugins": ["@sanity/vision"]
    }
  },
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    },
    {
      "implements": "part:@sanity/dashboard/config",
      "path": "./config/dashboardConfig.js"
    }
  ]
}
